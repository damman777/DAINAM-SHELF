        "# if, elif, else\n",
        "\n",
        "age = int(input(\"Enter Your Age:\"))\n",
        "if (age < 18):\n",
        "  print(\"You are Child\")\n",
        "elif (age > 18):\n",
        "  print(\"You are Adult\")\n",
        "elif (age >= 18):\n",
        "  print(\"You are Eligible\")\n",
        "else:\n",
        "  print(\"Go\")\n"
